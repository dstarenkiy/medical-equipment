- var news = ["Новинка", "Лучшая цена", "Хит продаж"];
	mixin product(catalog)
		+b.product
			+e.IMG.img(src=catalog.productImg)
			each name in news
				+e.stickernew= catalog.sticker				
			+e.caption
				+e.aparat= catalog.productCat
				+e.name 
					+e.a(href='#')= catalog.productName
				+e.for= catalog.productFor
			+e.priceblock
				+e.oldprice= catalog.productPriceOld
				+e.price= catalog.productPrice
				+e.a(href='#')
					+e.BUTTON(type='button').buy= catalog.buttonBuy